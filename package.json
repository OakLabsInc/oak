{
  "name": "oak",
  "version": "3.1.3",
  "description": "A very opinionated kiosk framework based on Electron",
  "engines": {
    "node": "7.4.0"
  },
  "scripts": {
    "coverage": "node node_modules/.bin/istanbul cover node_modules/.bin/tape test/*.js || true",
    "coveralls": "npm run-script coverage && node node_modules/.bin/coveralls < coverage/lcov.info && rm -rf coverage/ || true",
    "test": "node_modules/.bin/standard && bin/entrypoint test/app.test.js",
    "prepublish": "npm t"
  },
  "main": "index.js",
  "bin": {
    "oak": "bin/entrypoint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OakLabsInc/oak.git"
  },
  "license": "Apache-2.0",
  "author": "Flynn Joffray <nucleardreamer@gmail.com>",
  "bugs": {
    "url": "https://github.com/OakLabsInc/oak/issues"
  },
  "homepage": "https://github.com/OakLabsInc/oak",
  "dependencies": {
    "commander": "~2.9.0",
    "electron": "1.6.11",
    "eventemitter2": "~0.4.14",
    "lodash": "~4.17.2",
    "minimatch": "~3.0.4",
    "oak-tools": "~0.5.0",
    "uuid": "~2.0.2"
  },
  "devDependencies": {
    "coveralls": "~2.13.1",
    "ip": "~1.1.5",
    "istanbul": "~0.4.5",
    "standard": "~10.0.2",
    "tape": "~4.6.3"
  },
  "keywords": [
    "oak",
    "oakos",
    "IoT",
    "kiosk",
    "electron",
    "docker",
    "docker electron",
    "raspi kiosk",
    "rpi",
    "display"
  ],
  "standard": {
    "ignore": [
      "*/tmp/",
      "examples/"
    ]
  }
}
